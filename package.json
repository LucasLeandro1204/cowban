    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.14.4",
    "objection": "^1.0.0",
    "sqlite3": "^3.1.13",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-spies": "^1.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
